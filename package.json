{
    "name": "bodymass",
    "version": "1.0.0",
    "description": "Create a web application to calculate the body mass index given the mass and height of the user. The application must indicate the category accordingly to the standard. You should register as a user in order to use the application and calculate your body mass index.",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=production npm run build:prod",
        "hot": "NODE_ENV=hot npm run build:dev",
        "dev": "npm run build:dev",
        "build:dev": "webpack && node index.js",
        "build:prod": "webpack -p && node index.js"
    },
